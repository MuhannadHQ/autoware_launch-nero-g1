<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="vehicle_id" default="$(env VEHICLE_ID default)"/>
  
  <!-- Bridge node -->
  <node
    pkg="yhs_autoware_bridge"
    exec="bridge_node"
    name="bridge_node"
    output="screen"
  />

  <!-- YHS Can Control -->
  <include file="$(find-pkg-share yhs_can_control)/launch/yhs_can_control.launch.py">
  </include>

</launch>
