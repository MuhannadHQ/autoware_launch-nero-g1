<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="vehicle_id" default="$(env VEHICLE_ID default)"/>

  <node
    pkg="yhs_autoware_bridge"
    exec="bridge_node"
    name="bridge_node"
    output="screen"
  />

  <!-- Include yhs_can_control Python launch -->
  <include file="$(find yhs_can_control)/launch/yhs_can_control_launch.py">
    <arg name="params_file" value="$(find yhs_can_control)/params/cfg.yaml"/>
  </include>


</launch>
